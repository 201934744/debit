[Desktop Entry]
Type=Application
Encoding=UTF-8
Name=XilEdit
Comment=Xilinx bitstream browser
Exec=xiledit -i %f
Icon=ulogic_icon_32.png
Terminal=false
MimeType=application/x-xilinx-bitstream;
Categories=Development;Viewer;
Actions=ToPDF;

[Desktop Action ToPDF]
Exec=bit2pdf -i %f -o %f.pdf
Name=XilEdit (produce PDF)
